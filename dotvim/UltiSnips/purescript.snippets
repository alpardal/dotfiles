snippet mod "module with file name" b
module ${1:`!v tr(fnamemodify(expand("%"), ":~:.:r:s?src/??"), "/", ".")`} (
  $2
) where
$0
endsnippet

snippet imp "import" b
import ${1:ModuleName} (${2:..})
$0
endsnippet

snippet f "function definition" b
${1:name} :: ${2:type_sig}
$1 ${3:args} = $4
endsnippet

snippet a "arrow exp: a -> b" i
${1:a} -> ${2:b}
endsnippet
