snippet mod "module with file name" b
module $1`!v expand("%:t:r")` exposing (${2:..})
$0
endsnippet

snippet imp "import" b
import ${1:ModuleName} exposing (${2:..})
$0
endsnippet

snippet im "import simple" b
import ${1:ModuleName}
$0
endsnippet

snippet def "function definition" b
${1:name} : ${2:type_sig}
$1 ${3:args }= $4
endsnippet
